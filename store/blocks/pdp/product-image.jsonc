{
  "html#flex-layout.row#product-image": {
    "props": {
      "testId": "product-images"
    },
    "children": ["product-images"]
  },

  "product-images": {
    "props": {
      "aspectRatio": {
        "desktop": "auto",
        "phone": "1:1"
      },
      "displayThumbnailsArrows": false,
      "showPaginationDots": false,
      "thumbnailsOrientation": "horizontal"
    }
  }
}
